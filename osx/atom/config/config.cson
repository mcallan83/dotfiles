"*":
  "advanced-open-file":
    createDirectories: true
    createFileInstantly: true
  "atom-beautify":
    general:
      _analyticsUserId: "8b2c0325-f9e2-4d0c-adc3-6622ebe9bf93"
    php: {}
  core:
    automaticallyUpdate: false
    disabledPackages: [
      "atomic-chrome"
      "change-case"
      "expose"
      "autocomplete-php"
      "php-composer-completion"
    ]
    excludeVcsIgnoredPaths: false
    themes: [
      "one-dark-ui"
      "cobalt2-syntax"
    ]
  editor:
    fontSize: 17
    showIndentGuide: true
    showInvisibles: true
  "exception-reporting":
    userId: "8adfcc57-bd96-b2ab-bba2-1cc3e1699907"
  linter:
    lintOnFlyInterval: 2000
    showErrorInline: false
  "linter-eslint":
    lintHtmlFiles: true
  minimap:
    absoluteMode: true
    adjustAbsoluteModeHeight: true
    plugins:
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      linterDecorationsZIndex: 0
      pigments: true
      pigmentsDecorationsZIndex: 0
  "project-manager":
    closeCurrent: true
    environmentSpecificProjects: true
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  "tool-bar": {}
  "tool-bar-almighty": {}
  welcome:
    showOnStartup: false
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
